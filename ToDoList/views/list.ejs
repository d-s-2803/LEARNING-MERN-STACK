<%- include("header") -%>
  <div class="box" id="heading">
    <h1><%= dayenter %> List</h1>
  </div>
  <div class="box">
    <ul>
      <% for ( var i = 0 ; i < newlistitem.length ; i++){ %>
      <div class="item">
        <input type="checkbox">
        <p><%= newlistitem[i] %>
        <p>
      </div>
      <% } %>
    </ul>
    <form class="" action="/" method="post">
      <input type="text" name="newItem" placeholder="New Item" autocomplete="off">
      <button type="submit" name="list" value = <%= dayenter %>>+</button>
    </form>
  </div>

  <%- include("footer") -%>
